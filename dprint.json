{
  "typescript": {
    "semiColons": "asi",
    "trailingCommas": "never"
  },
  "json": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/dist",
    "deps"
  ],
